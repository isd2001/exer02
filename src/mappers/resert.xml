<?xml version="1.0" encoding="UTF-8" ?> 
<!DOCTYPE mapper  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="resert">
	<insert id="addboardMap" parameterType="java.util.HashMap">
		insert into board values(board_seq.nextval,#{logid},#{title},sysdate,#{content},0,null)
	</insert>
	<select id="getAllDate" resultType="java.util.HashMap">
		select * from board order by no desc 
	</select>
	<select id="getOneView" resultType="java.util.HashMap" parameterType="java.lang.String">
		select * from board where no=#{no}
	</select>
	<update id="upGoodByNo" parameterType="java.lang.String">
		update board set good= good+1 where no=#{no}
	</update>
	<insert id="addGoodlog" parameterType="java.util.HashMap">
		insert into goodlog values(#{logid},#{no})
	</insert>
	
</mapper>